<config>
	<step>
		<class>
			com.sina.website.UserTagDriver
		</class>
		<field static="true">
			<name>INPUT_FILE</name>
			<value>/file/suda/suda_lzo</value>
		</field>
		<field static="true">
			<name>HOST_PATH</name>
			<value>/user/fengming/host.txt</value>
		</field>
		<field static="true">
			<name>URL_REDIS_NAME</name>
			<value>REDIS2</value>
		</field>
		<field static="true">
			<name>URL_REGION</name>
			<value>URL_REGION</value>
		</field>
		<field static="true">
			<name>USER_REDIS_NAME</name>
			<value>REDIS1</value>
		</field>
		<field static="true">
			<name>USER_REGION</name>
			<value>USER_REGION</value>
		</field>
		<field static="true">
			<name>PATTERN_STRING</name>
			<value>http://.*/2012[0-9-/]{4,6}/[0-9]{7,}\\.(html|shtml)</value>
		</field>
		<field static="true">
			<name>TRY_TIMES</name>
			<value>1</value>
		</field>
		<field static="true">
			<name>USER_TAG_THRESHOLD</name>
			<value>20</value>
		</field>				
		<field static="true">
			<name>JEDIS_USER_EXPIRED</name>
			<value>604800</value>
		</field>				
		<method>
			<name>run</name>
		</method>
		<sleep>10000</sleep>
	</step>
	 
</config>
